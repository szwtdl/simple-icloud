{
    "name": "szwtdl/simple-icloud",
    "description": "icloud icloud backup data view",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.4",
        "nesbot/carbon": "2.62.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan": "^1.8",
        "jetbrains/phpstorm-attributes": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.11"
    },
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "SimpleIcloud\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "szwtdl",
            "email": "szpengjian@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "SimpleIcloud\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
